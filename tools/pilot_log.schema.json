{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "required": ["ts","user","mode","resume","seconds","thumb","reason"],
    "properties": {
      "ts": {"type":"string"},
      "user": {"type":"string"},
      "mode": {"type":"string","enum":["before","after"]},
      "resume": {"type":"string"},
      "seconds": {"type":"number","minimum":0},
      "thumb": {"type":"string","enum":["up","down"]},
      "reason": {"type":"string"}
    },
    "additionalProperties": false
  }
  